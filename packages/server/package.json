{
  "name": "@cubecobra/server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "copy-prod-client": "mkdir -p dist/server/src/client/ && cp -r ../client/dist/* dist/server/src/client/",
    "copy-dev-client": "rm -rf src/client && mkdir -p src/client/  && cp -r ../client/dist/* src/client/",
    "copy-client": "npm run copy-prod-client && npm run copy-dev-client",
    "copy-public": "copyfiles -u 0 -a 'public/**' dist/server/",
    "fix-external-paths": "npx resolve-tspaths && node scripts/fixExternalPaths.js",
    "build": "npm run write-git-info && npx tsc && npm run fix-external-paths && npm run copy-client && copyfiles -u 1 'src/**/*.pug' dist/server/src && npm run copy-public && copyfiles -u 2 'src/emails/**/*' dist/server/src/emails",
    "type-check": "npx tsc --noEmit",
    "start": "node server.js",
    "dev": "npm run write-git-info && nodemon -r tsconfig-paths/register src/index.ts",
    "clean": "rm -rf dist/ node_modules/ private/ ./src/client",
    "test": "npm run write-git-info && npx jest --coverage=false",
    "test:watch": "npm run write-git-info && npx jest --watch --coverage=false",
    "test:coverage": "npm run write-git-info && npx jest --coverage",
    "test:ci": "npm run write-git-info && npx jest --coverage --coverageReporters json-summary text",
    "write-git-info": "ts-node scripts/write-git-info.ts"
  },
  "keywords": [],
